@model MvcCv.Models.Entity.TblAdmin


@{
    ViewBag.Title = "AdminCall";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4>Şifre Guncelleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Admin Id")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Kullanıcı Adı")
    @Html.TextBoxFor(x => x.UserName, new { @class = "form-control" })
    <br />
    @Html.Label("Şifre")
    @Html.TextBoxFor(x => x.Password, new { @class = "form-control" })
    <button class="btn btn-info">Guncelle</button>
</form>
<br />
<a class="btn btn-info" href="/Admin/Index">İptal et</a>