@model MvcCv.Models.Entity.TblSkills

@{
    ViewBag.Title = "SkillCall";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yetenek Guncelleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Yeteneğim")
    @Html.TextBoxFor(x => x.Skill, new { @class = "form-control" })
    <br />
    @Html.Label("Oranı")
    @Html.TextBoxFor(x => x.Ratio, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Guncelle</button>
</form>