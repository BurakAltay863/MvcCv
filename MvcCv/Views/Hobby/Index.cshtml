@using MvcCv.Models.Entity
@model List<TblHobby>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Index</h2>
<br />
<form method="post">
    @foreach (var item in Model)
    {
        <label>Açıklama1</label>
        <textarea rows="5" class="form-control" name="Descreption1">@item.Descreption1</textarea>
        <label>Açıklama2</label>
        <textarea rows="5" class="form-control" name="Descreption2">@item.Descreption2</textarea>
    }
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>
