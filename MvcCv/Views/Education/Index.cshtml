@using MvcCv.Models.Entity
@model List<TblEducation>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h2>Eğitim Listesi</h2>

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Başlık</th>
        <th>AltBaşlık</th>
        <th>Subtitle2</th>
        <th>GNO</th>
        <th>Date</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>@item.ID</td>
    <td> @item.Title</td>
    <td>@item.Subtitle1</td>
    <td>@item.Subtitle2</td>
    <td>@item.GNO</td>
    <td>@item.Date</td>
    <td>
        <a class="btn btn-danger btn-sm" href="/Education/EducationDelete/@item.ID/">
            <i class="fas fa-trash"></i>
            Sil
        </a>

    </td>
    <td>
        <a class="btn btn-info btn-sm" href="/Education/EducationCall/@item.ID/">
            <i class="fas fa-pencil-alt"></i>
            Düzenle
        </a>
    </td>

</tr>
    }
</table>
<a href="/Education/EducationAdd/" class="btn btn-primary">Yeni Eğitim Ekle</a>


