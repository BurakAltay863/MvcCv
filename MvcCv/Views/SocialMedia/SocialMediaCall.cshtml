@model MvcCv.Models.Entity.TblSocialMedia
@{
    ViewBag.Title = "SocialMediaCall";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h4>Medya Guncelleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Adı")
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    @Html.Label("Link")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Ikon")
    @Html.TextBoxFor(x => x.Ikon, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Guncelle</button>
</form>
<br />
<a class="btn btn-info" href="/SocailMedia/Index">İptal et</a>

