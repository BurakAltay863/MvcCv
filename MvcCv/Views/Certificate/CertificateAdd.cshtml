@model MvcCv.Models.Entity.TblCertificate

@{
    ViewBag.Title = "CertificateAdd";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h4>Eğitim Ekleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />
    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Descreption, new { @class = "form-control" })
    <button class="btn btn-primary">Ekle</button>
</form>
<br />
<a class="btn btn-info" href="/Certificate/Index">İptal et</a>
